   IF NOT Empty(@subCountry) THEN
      SET @country = @subCountry
      SET @language = @subLanguage
      IF Empty(@sublanguage) AND @country == "CA" THEN
         SET @language = "English"
      ENDIF 
   ENDIF
   
   /* Set Language for Journey emails. Note that Double Opt In uses different fields so it uses the Sub method for MC Connect Emails above */
   IF @emailType == "journey" AND @emailVersion != "DoubleOpt" THEN 
      SET @language = AttributeValue('Account:PersonContact:Language__c')
      SET @country = AttributeValue('Account:PersonContact:PreferredCountry__c')

      IF EMPTY(@language) THEN
         SET @language = AttributeValue('Contact:Language__c')
      ENDIF
      IF EMPTY(@country) THEN
         SET @country = AttributeValue('Contact:PreferredCountry__c')
      ENDIF
   ENDIF

   /* Default to en-CA if country is Canada, and en-US if no language or country value for all others */
   IF Empty(@language) OR Empty(@country) THEN  
      IF @country == "CA" THEN
         SET @language = "en"
      ELSE
         Set @country = "US"
         SET @language = "en"
      ENDIF
   ENDIF

   SET @isEU = "false" /* Default to false. This value is used to determine if the subscriber is in the EU */
   SET @hashrows = LookupRows("ENT.EU_Countries", "Countries", @country)
   IF RowCount(@hashrows) > 0 THEN
