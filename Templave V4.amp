                     IF (@hideFooterLinks != "Y" AND (@emailType == "journey" OR @emailType == "marketing" OR @emailType == "trigger" OR Empty(@emailType))) OR (@hideFooterLinks == "N" AND @emailType == "Transactional") THEN 
